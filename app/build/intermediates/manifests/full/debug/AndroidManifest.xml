<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.christianphan.mednew"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        <activity android:name="com.christianphan.mednew.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.christianphan.mednew.LoginActivity" />
        <activity android:name="com.christianphan.mednew.Newuser_Login" />
        <activity android:name="com.christianphan.mednew.UserCheckInPage" />
        <activity android:name="com.christianphan.mednew.ListOfActivities" />
        <activity android:name="com.christianphan.mednew.GraphActivity" />
    </application>

</manifest>